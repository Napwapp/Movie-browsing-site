generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MovieTambahan {
  id            Int      @id @default(autoincrement())
  title         String
  poster_path   String
  vote_average  Float
  release_date  DateTime
  genres        Json
  overview      String
}
